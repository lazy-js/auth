{
  "name": "@lazy-js/auth",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run dev:esm && npm run dev:cjs",
    "dev:esm": "tsc --watch --project tsconfig.json",
    "dev:cjs": "tsc --watch --project tsconfig.cjs.json",
    "start": "node dist/cjs/index.js",
    "prepublishOnly": "npm run build",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project tsconfig.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "test": "tsc --project tsconfig.test.json && vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@keycloak/keycloak-admin-client": "^26.2.5",
    "@lazy-js/mongo-db": "github:lazy-js/mongo-db",
    "@lazy-js/server": "github:lazy-js/server",
    "@lazy-js/utils": "github:lazy-js/utils",
    "jose": "^6.0.11",
    "zod": "^4.1.5"
  },
  "type": "commonjs",
  "devDependencies": {
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
